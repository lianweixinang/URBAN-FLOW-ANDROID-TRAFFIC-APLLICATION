                            android:id="@+id/t3"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="30.0"
                            android:textColor="#6C6A61"
                            android:textSize="35dp"
                            android:textStyle="bold"
                            android:layout_marginLeft="210dp" />
