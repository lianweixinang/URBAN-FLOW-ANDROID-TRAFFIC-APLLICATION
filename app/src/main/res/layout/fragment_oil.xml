                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:text="下週油價"
                    android:layout_gravity="center"
                    android:textColor="#000000"
                    android:textSize="25dp" />
            </androidx.cardview.widget.CardView>
            <androidx.cardview.widget.CardView
                android:layout_width="match_parent"
                android:layout_height="260dp"
                android:layout_marginTop="20dp"
                app:cardCornerRadius="15dp">

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="260dp"
                    android:background="#FFFFFF"
                    android:padding="10dp">
                    <ImageView
                        android:id='@+id/image922'
                        android:layout_width="match_parent"
                        android:layout_height="80dp"
                        android:layout_marginTop="20dp"
                        android:layout_marginRight="300dp"
                        app:cardCornerRadius="10dp"
                        app:srcCompat="@drawable/group_223" />

                    <ImageView
                        android:id='@+id/image952'
                        android:layout_width="match_parent"
                        android:layout_height="80dp"
                        app:cardCornerRadius="10dp"
                        android:layout_marginTop="20dp"
                        android:layout_marginRight="100dp"
                        app:srcCompat="@drawable/_5" />

                    <ImageView
                        android:id='@+id/image982'
                        android:layout_width="match_parent"
                        android:layout_height="80dp"
                        android:layout_marginTop="20dp"
                        android:layout_marginLeft="100dp"
                        app:cardCornerRadius="10dp"
                        app:srcCompat="@drawable/group_222" />

                    <ImageView
                        android:id='@+id/dieselicon2'
                        android:layout_width="match_parent"
                        android:layout_height="80dp"
                        android:layout_marginTop="20dp"
                        android:layout_marginLeft="300dp"
                        app:cardCornerRadius="10dp"
                        app:srcCompat="@drawable/group_224" />
                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="80dp"
                        android:layout_marginTop="110dp"
                        android:background="#FFFFFF"
                        android:padding="5dp">
                        <TextView
                            android:id="@+id/t6"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="30.0"
                            android:textColor="#6C6A61"
                            android:textSize="35dp"
                            android:textStyle="bold"
                            android:layout_marginLeft="10dp" />
                        <TextView
                            android:id="@+id/t7"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="30.0"
                            android:textColor="#6C6A61"
                            android:textSize="35dp"
                            android:textStyle="bold"
                            android:layout_marginLeft="110dp" />
                        <TextView
                            android:id="@+id/t8"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="30.0"
                            android:textColor="#6C6A61"
                            android:textSize="35dp"
                            android:textStyle="bold"
                            android:layout_marginLeft="210dp" />
                        <TextView
                            android:id="@+id/t9"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="30.0"
                            android:textColor="#6C6A61"
                            android:textSize="35dp"
                            android:textStyle="bold"
                            android:layout_marginLeft="310dp" />
                    </RelativeLayout>
                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="80dp"
                        android:layout_marginTop="160dp"
                        android:background="#FFFFFF"
                        android:padding="5dp">
                        <TextView
                            android:id="@+id/t10"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="+0.0"
                            android:textColor="#145DA0"
                            android:textSize="25dp"
                            android:textStyle="bold"
                            android:layout_marginLeft="10dp" />
                        <TextView
                            android:id="@+id/t11"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="+0.0"
                            android:textColor="#145DA0"
                            android:textSize="25dp"
                            android:textStyle="bold"
                            android:layout_marginLeft="110dp" />
                        <TextView
                            android:id="@+id/t12"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="+0.0"
                            android:textColor="#145DA0"
                            android:textSize="25dp"
                            android:textStyle="bold"
                            android:layout_marginLeft="210dp" />
                        <TextView
                            android:id="@+id/t13"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="+0.0"
                            android:textColor="#145DA0"
                            android:textSize="25dp"
                            android:textStyle="bold"
                            android:layout_marginLeft="310dp" />
                    </RelativeLayout>
                </RelativeLayout>
            </androidx.cardview.widget.CardView>
        </LinearLayout>
    </androidx.constraintlayout.widget.ConstraintLayout></FrameLayout>