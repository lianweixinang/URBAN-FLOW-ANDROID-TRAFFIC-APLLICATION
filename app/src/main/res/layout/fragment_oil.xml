<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/fragment_oil"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#EDEFF4">
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        tools:context="ntutifm.game.google.ui.notification.NotificationFragment">
        <LinearLayout
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <androidx.cardview.widget.CardView
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_marginTop="20dp"
                app:cardCornerRadius="15dp"
                app:cardBackgroundColor="#FFFFFF">
                <ImageView
                    android:id="@+id/gasIcon1"
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:layout_marginLeft="125dp"
                    android:layout_marginTop="5dp"
                    android:src='@drawable/baseline_local_gas_station_24'
                    app:cardCornerRadius="10dp" />
                <TextView
                    android:id="@+id/t0"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:text="本週油價"
                    android:textColor="#000000"
                    android:textSize="25dp"
                    android:layout_gravity="center"/>
            </androidx.cardview.widget.CardView>
            <androidx.cardview.widget.CardView
                android:layout_width="match_parent"
                android:layout_height="220dp"
                android:layout_marginTop="20dp"
                app:cardCornerRadius="15dp"
                app:cardBackgroundColor="#FFFFFF">

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="220dp"
                    android:background="#FFFFFF"
                    android:padding="5dp">
                    <ImageView
                        android:id='@+id/image92'
                        android:layout_width="match_parent"
                        android:layout_height="80dp"
                        android:layout_marginTop="20dp"
                        android:layout_marginRight="300dp"
                        app:cardCornerRadius="10dp"
                        app:srcCompat="@drawable/group_223" />

                    <ImageView
                        android:id='@+id/image95'
                        android:layout_width="match_parent"
                        android:layout_height="80dp"
                        app:cardCornerRadius="10dp"
                        android:layout_marginTop="20dp"
                        android:layout_marginRight="100dp"
                        app:srcCompat="@drawable/_5" />

                    <ImageView
                        android:id='@+id/image98'
                        android:layout_width="match_parent"
                        android:layout_height="80dp"
                        android:layout_marginTop="20dp"
                        android:layout_marginLeft="100dp"
                        app:cardCornerRadius="10dp"
                        app:srcCompat="@drawable/group_222" />

                    <ImageView
                        android:id='@+id/dieselicon'
                        android:layout_width="match_parent"
                        android:layout_height="80dp"
                        android:layout_marginTop="20dp"
                        android:layout_marginLeft="300dp"
                        app:cardCornerRadius="10dp"
                        app:srcCompat="@drawable/group_224" />

                    <RelativeLayout
                        android:layout_width="400dp"
                        android:layout_height="80dp"
                        android:layout_marginTop="110dp"
                        android:background="#FFFFFF"
                        android:padding="5dp">
                        <TextView
                            android:id="@+id/t1"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="30.0"
                            android:textColor="#6C6A61"
                            android:textSize="35dp"
                            android:textStyle="bold"
                            android:layout_marginLeft="10dp" />

                        <TextView
                            android:id="@+id/t2"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="110dp"
                            android:text="30.0"
                            android:textColor="#6C6A61"
                            android:textSize="35dp"
                            android:textStyle="bold" />

