<?xml version="1.0" encoding="utf-8"?>
<com.google.android.material.card.MaterialCardView
    android:id="@+id/root"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_marginTop="5dp"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <androidx.cardview.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="300dp"
        android:layout_marginTop="20dp"
        app:cardCornerRadius="15dp"
        app:cardBackgroundColor="#EFEFEF">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="#EFEFEF"
            android:padding="5dp">
            <ImageView
                android:id='@+id/image92'
                android:layout_width="match_parent"
                android:layout_height="80dp"
                android:layout_marginTop="35dp"
                android:layout_marginRight="300dp"
                app:cardCornerRadius="10dp"
                app:srcCompat="@drawable/group_223" />

            <ImageView
                android:id='@+id/image95'
                android:layout_width="match_parent"
                android:layout_height="80dp"
                app:cardCornerRadius="10dp"
                android:layout_marginTop="35dp"
                android:layout_marginRight="100dp"
                app:srcCompat="@drawable/_5" />

            <ImageView
                android:id='@+id/image98'
                android:layout_width="match_parent"
                android:layout_height="80dp"
                android:layout_marginTop="35dp"
                android:layout_marginLeft="100dp"
                app:cardCornerRadius="10dp"
                app:srcCompat="@drawable/group_222" />

            <ImageView
                android:id='@+id/dieselicon'
                android:layout_width="match_parent"
                android:layout_height="80dp"
                android:layout_marginTop="35dp"
                android:layout_marginLeft="300dp"
                app:cardCornerRadius="10dp"
                app:srcCompat="@drawable/group_224" />

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="100dp"
                android:layout_marginTop="150dp"
                android:background="#B7B7B7"
                android:padding="5dp">
                <TextView
                    android:id="@+id/t1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="28.5"
                    android:textColor="#ffff"
                    android:textSize="35dp"
                    android:textStyle="bold"
                    android:layout_marginLeft="10dp" />

                <TextView
                    android:id="@+id/t2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="110dp"
                    android:text="29.5"
                    android:textColor="#ffff"
                    android:textSize="35dp"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/t3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="30.5"
                    android:textColor="#ffff"
                    android:textSize="35dp"
                    android:textStyle="bold"
                    android:layout_marginLeft="210dp" />
                <TextView
                    android:id="@+id/t4"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="31.5"
                    android:textColor="#ffff"
                    android:textSize="35dp"
                    android:textStyle="bold"
                    android:layout_marginLeft="300dp" />
            </RelativeLayout>
        </RelativeLayout>

    </androidx.cardview.widget.CardView>
</com.google.android.material.card.MaterialCardView>
